{
  "name": "jkt",
  "version": "2.3.3",
  "description": "Simple and efficient library for JSON data structure",
  "main": "./src/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require vars.js --timeout 15000 --recursive --slow 1 test/",
    "test:watch": "NODE_ENV=test mocha --watch --require vars.js --timeout 15000 --recursive --slow 1 test/",
    "test:ts": "NODE_ENV=test mocha --require vars-ts.js --require esm --timeout 15000 --recursive --slow 1 test/",
    "test:ts:watch": "NODE_ENV=test mocha --watch --require vars-ts.js --require esm --timeout 15000 --recursive --slow 1 test/",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "coverage": "type-coverage --project tsconfig.json --detail",
    "type-coverage:watch": "watch 'npm run coverage' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slaveofcode/jkt.git"
  },
  "keywords": [
    "json",
    "structure",
    "json",
    "library",
    "json",
    "data",
    "structure",
    "parser",
    "serializer"
  ],
  "author": "Aditya Kresna Permana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slaveofcode/jkt/issues"
  },
  "homepage": "https://github.com/slaveofcode/jkt#readme",
  "devDependencies": {
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.12.18",
    "chai": "^4.1.2",
    "chai-things": "^0.2.0",
    "esm": "^3.0.84",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "type-coverage": "^1.6.2",
    "typescript": "^3.2.2",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.11",
    "moment": "^2.20.1",
    "shortid": "^2.2.8"
  }
}
